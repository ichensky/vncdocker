#!/bin/sh

# docker container home dir will build from:
home=home

# docker container will share home dir:
share=share

# docker image name
imagename=ichensky/vncdocker:dwm_audacity

# docker container name
containername=vncdocker_dwm_audacity

# user name inside of docker container
username=john

# starting gui app inside of docker container
start=x-window-manager
